FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      dynamicMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dynamicFvMesh   multiSolidBodyMotionFvMesh;

motionSolverLibs ( "libfvMotionSolvers.so" );

multiSolidBodyMotionFvMeshCoeffs
{
	rotating_xp-cells
	{
		solidBodyMotionFunction rotatingMotion;
		rotatingMotionCoeffs
		    {
			origin        (0.0425 0 0.1);
			axis          (0 0 1);
			omega         10.5; // rad/s
		    }
	}

	rotating_xn-cells
	{
		solidBodyMotionFunction rotatingMotion;
		rotatingMotionCoeffs
		    {
			origin        (-0.0425 0 0.1);
			axis          (0 0 -1);
			omega         10.5; // rad/s
		    }
	}
		
}

