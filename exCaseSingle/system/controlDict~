FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleDyMFoam;

startFrom       latestTime;

//startTime       0;

stopAt          endTime;

endTime         5;

deltaT          1e-6;

writeControl    adjustableRunTime;

writeInterval   1e-2;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           20.0;

libs
(
    "libincompressibleTurbulenceModel.so"
    "libincompressibleRASModels.so"
);

functions
{
    forces
    {
        type                forces;
        libs		    ("libforces.so");
        patches             (blades);
        CofR                (0 0 0);
        rho	            rhoInf;
	log		    true;
        rhoInf              1.225;
        outputControl       timeStep;
        outputInterval      100;
    }

};


