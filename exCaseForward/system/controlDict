FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


application     pimpleDyMFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1e5;

deltaT   	2.36e-4; 

writeControl    adjustableRunTime;

writeInterval   2.36e-3; 

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression off;

timeFormat      general;

timePrecision   10;

runTimeModifiable true;

adjustTimeStep  no;

maxCo           10;

functions 
{

//	yPlus
//	{
//	    type        yPlus;
//	    libs        ("libfieldFunctionObjects.so");
//	}

    forcesBlade1
    {
        type forces;
        functionObjectLibs ("libforces.so");
        outputControl timeStep;
        outputInterval 1;
        patches (blade1);
	//writeFields yes;

        rhoName rhoInf;
        log false;
        CofR (0 -0.0425 0);
	rho	rhoInf;
	rhoInf	1e3;
    }

